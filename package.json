{
  "name": "data-hunter-pro",
  "version": "1.3.6",
  "description": "Professional web data hunting tool. Hunt and extract any website data with advanced AI-powered detection and smart extraction features.",
  "private": true,
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production && node scripts/post-build.js",
    "build:dev": "webpack --mode development && node scripts/post-build.js",
    "clean": "rimraf dist",
    "install:extension": "npm run build && echo 'Extension built! Load the dist folder in Chrome as an unpacked extension.'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@tanstack/react-table": "^8.10.7",
    "papaparse": "^5.4.1",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
